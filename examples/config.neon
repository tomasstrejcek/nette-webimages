extensions:
	webimages: DotBlue\WebImages\Extension

webimages:
	providers:
		- DefaultImageProvider(%wwwDir%)
		- FakeImageProvider

	routes:
		- 'images/<id>-<width>x<height>.jpg'
		- 'images/<id>-<width>x<height>.png'

	rules:
		- [width: 300, height: 200]
		- [width: 50, height: 30]

	image:
		namespace:
			type:
				size: [1200, null]
				quality: 80
				resize: default
